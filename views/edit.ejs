<div class="wrapper">
    <h1>Edit restaurants</h1>
    <div class="container">
        <form class="form" action="/<%=id%>?_method=PUT" method="POST">
            <div class="inputParent">
                <label class='font' for="name">Name:</label>
                <input class='input font' type='text' name='name' value="<%=name%>" />
            </div>
            <input class='button font submit' type="submit" />
            <a class='button font cancel' href="/biz">Cancel</a>
        </form>
    </div>
</div>