<link rel="stylesheet" type="text/css" href="/style.css">
<div class="wrapper">
    <div class="container">
        <h2>Let's Shake Things Up!</h2>
        <form class="form" action="/<%=id%>?_method=PUT" method="POST">
            <div class="inputParent">
                <label class='font' for="name">Name:</label>
                <input class='input font' type='text' name='name' value="<%=this.name%>" />
                <label class='font' for="cuisine">Cuisine:</label>
                <input class='input font' type='text' name='came' value="<%=cuisine%>" />
                <label class='font' for="image"> Link to Image</label>:</label>
                <input class='input font' type='text' name='image' value="<%=this.image%>" />
                <input class='submit-button font submit' type="submit" />
                <a class='cancel-button font cancel' href="/biz">Cancel</a>
            </div>
        </form>
    </div>
</div>