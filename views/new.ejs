<link rel="stylesheet" type="text/css" href="/style.css">
<div class="wrapper">
    <div class="container">
        <h1>Add Restaurant</h1>
        <form class="form" action="/add" method="POST">
            <div class="inputParent">
                <label class='font' for="name">Name:</label>
                <input class='input font' type='text' name='name' />
                <label class='font' for="cuisine">Cuisine:</label>
                <input class='input font' type='text' name='cuisine' />
            </div>

            <input class='button font submit' type="submit" />
            <a class='button font cancel' href="/biz">Cancel</a>
        </form>
    </div>
</div>